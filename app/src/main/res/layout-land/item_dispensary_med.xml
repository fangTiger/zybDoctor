<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    android:gravity="center_vertical"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/tv_dis_med_item_index"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:maxLines="1"
        android:text="序号" />

    <TextView
        android:id="@+id/tv_dis_med_item_code"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:maxLines="1"
        android:text="药品编号" />

    <TextView
        android:id="@+id/tv_dis_med_item_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center"
        android:maxLines="1"
        android:text="药品名称" />

    <TextView
        android:id="@+id/tv_dis_med_item_type"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:maxLines="1"
        android:text="药品分类" />

    <TextView
        android:id="@+id/tv_dis_med_item_add"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:maxLines="1"
        android:text="药品产地" />

    <TextView
        android:id="@+id/tv_dis_med_item_supply"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:maxLines="1"
        android:text="直供" />

    <TextView
        android:id="@+id/tv_dis_med_item_store"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:maxLines="1"
        android:text="库存" />

    <TextView
        android:id="@+id/tv_dis_med_item_num"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:maxLines="1"
        android:text="出库量" />

</LinearLayout>